<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>My Blog</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta name="author" content="" />
    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content="" />
    <meta property="og:image" content="" />
    <meta property="og:url" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:description" content="" />
    <meta name="twitter:title" content="" />
    <meta name="twitter:image" content="" />
    <meta name="twitter:url" content="" />
    <meta name="twitter:card" content="" />

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,700" rel="stylesheet">
    <link href="~/Content/PagedList.css" rel="stylesheet" type="text/css">


    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/Content/icomoon.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/bootstrap.css">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/Content/magnific-popup.css">
    <!-- Flexslider  -->
    <link rel="stylesheet" href="~/Content/flexslider.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/Content/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/owl.theme.default.min.css">
    <!-- Theme style  -->
    <link rel="stylesheet" href="~/Content/style.css">
    <!-- Modernizr JS -->
    <script src="~/Scripts/modernizr-2.6.2.min.js"></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <div class="colorlib-loader"></div>
    <div id="page">
        <nav class="colorlib-nav" role="navigation">
            <div class="top-menu">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-2">
                            <div id="colorlib-logo">
                                @Html.ActionLink("Information Technology", "Index", "Home")
                            </div>
                        </div>
                        <div class="col-xs-10 text-right menu-1">
                            <ul>
                                <li class="active">
                                    @Html.ActionLink("Home", "Index", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Blog", "Index", "BlogPosts")
                                </li>
                                <li>
                                    @Html.ActionLink("Comments", "Index", "Comments")
                                </li>
                                <li>
                                    @Html.ActionLink("About", "About", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Contact", "Contact", "Home")
                                </li>
                            </ul>
                            @Html.Partial("_LoginPartial")
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        @RenderBody()
        <div id="colorlib-instagram">
            <div class="row">
                <div class="col-md-12 col-md-offset-0 colorlib-heading text-center">
                    <br>
                    <br>
                    <h2>TOP TECHNOLOGY BLOGS ON THE INTERNET TODAY | TECHNOLOGY WEBSITES</h2>
                </div>
            </div>
            <div class="row">
                <div class="instagram-entry">
                    <a href="https://techcrunch.com/" class="instagram text-center" style="background-image: url(Pictures/link1.png);">
                        <strong><b>Tech Crunch</b></strong>
                    </a>
                    <a href="http://mashable.com/" class="instagram text-center" style="background-image: url(Pictures/link2.png);">
                        <strong> Mashable </strong>
                    </a>
                    <a href="https://www.technologyreview.com/" class="instagram text-center" style="background-image: url(Pictures/link3.png);">
                        <strong> Technology Review</strong>
                    </a>
                    <a href="http://readwrite.com/" class="instagram text-center" style="background-image: url(Pictures/link4.png);">
                        <strong>Read Write</strong>
                    </a>
                    <a href="https://venturebeat.com/" class="instagram text-center" style="background-image: url(Pictures/link5.png);">
                        <strong> Venture Beat</strong>
                    </a>
                    <a href="http://www.freetech4teachers.com/" class="instagram text-center" style="background-image: url(Pictures/link6.png);">
                        <strong> Free Tech For Teachers</strong>
                    </a>
                    <a href="http://www.makeuseof.com/" class="instagram text-center" style="background-image: url(Pictures/link7.png);">
                        <strong>Make Use Of</strong>
                    </a>
                    <a href="https://www.cnet.com/news/" class="instagram text-center" style="background-image: url(Pictures/link1.png);">
                        <strong>CNet</strong>
                    </a>
                </div>
            </div>
        </div>
        <footer id="colorlib-footer" role="contentinfo">
            <div class="container">
                <div class="row row-pb-md">
                    <div class="col-md-3">
                        <h2>Navigational</h2>
                        <p>
                            <ul class="colorlib-footer-links">
                                <li class="active"><i class="icon-check">@Html.ActionLink("Home", "Index", "Home")</i></li>
                                <li class="active"><i class="icon-check">@Html.ActionLink("Blog", "Index", "BlogPosts")</i></li>
                                <li class="active"><i class="icon-check">@Html.ActionLink("About", "About", "Home")</i></li>
                                <li class="active"><i class="icon-check">@Html.ActionLink("Contact", "Contact", "Home")</i></li>
                            </ul>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <h2>Recent Post</h2>
                        <div class="f-blog">
                            <img class="blog-img" style="background-image: url(Pictures/cover.jpg);"/>       
                                <div class="desc">
                                    <h3>
                                        @Html.ActionLink("Technology", "how-technology-is-going-over-everything", "Blog")
                                    </h3>
                                    <p class="admin"><span>23 September 2018</span></p>
                                </div>
                        </div>
                        <div class="f-blog">
                            <img class="blog-img" style="background-image: url(Pictures/cover3.jpg);" />
                            <div class="desc">
                                <h3>
                                    @Html.ActionLink("Drones", "how-drones-are-becoming-famous", "Blog")
                                </h3>
                                <p class="admin"><span>24 March 2018</span></p>
                            </div>
                        </div>
                        <div class="f-blog">
                            <img class="blog-img" style="background-image: url(Pictures/cover2.jpg);" />
                            <div class="desc">
                                <h3>
                                    @Html.ActionLink("Programming", "everything-is-calculated-programmed", "Blog")
                                </h3>
                                <p class="admin"><span>23 March 2018</span></p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <p>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <a href="https://colorlib.com" target="_blank">Colorlib</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            <a href="http://Google.com" target="_blank">Google</a>
                        </p>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    
    <div class="gototop js-top">
        <a href="#" class="js-gotop"><i class="icon-arrow-up2"></i></a>
    </div>

    <!-- jQuery -->
    <script src="~/Scripts/jquery.min.js"></script>
    <!-- jQuery Easing -->
    <script src="~/Scripts/jquery.easing.1.3.js"></script>
    <!-- Bootstrap -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- Waypoints -->
    <script src="~/Scripts/jquery.waypoints.min.js"></script>
    <!-- Flexslider -->
    <script src="~/Scripts/jquery.flexslider-min.js"></script>
    <!-- Owl carousel -->
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <!-- Magnific Popup -->
    <script src="~/Scripts/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/magnific-popup-options.js"></script>
    <!-- Main -->
    <script src="~/Scripts/main.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
